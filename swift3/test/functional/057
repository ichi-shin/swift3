. ./common

S3USER=tester

_s3_put /bucket -H "x-amz-acl:public-read"
_s3_get /bucket?acl -D - | _filter_curl xml

_s3_put /bucket?acl -H "x-amz-grant-write: emailAddress='xyz@amazon.com'" | _filter_curl xml
_s3_get /bucket?acl -D - | _filter_curl xml
